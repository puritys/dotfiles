alias ll='ls -la --time-style=long-iso --indicator-style=file-type --color=auto'
alias ls='ls --time-style=long-iso --indicator-style=file-type --group-directories-first --color=auto'
alias sudo='sudo -E '

alias timestamp='date +%s -d '


if [ -f ~/.alias_common ]; then
    source ~/.alias_common
fi

if [ -f ~/.alias_docker ]; then
    source ~/.alias_docker
fi

if [ -f ~/.alias_customized ]; then
    source ~/.alias_customized
fi
