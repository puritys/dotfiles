set convert-meta off 
set meta-flag on
set output-meta on

#設定按鍵 home與end不會變成 ~ ，而是跳成開頭與結尾
"\e[1~": beginning-of-line
"\e[4~": end-of-line
"\e[3~": delete-char

# for xterm
"\eOH": beginning-of-line
"\eOF": end-of-line

#"\e[5~": "history"
#F1
#"\eOP":\C-c
#"\e[2~": interrupt
#"\C-c"
# 允许命令行提示符转到下一行
set horizontal-scroll-mode Off

